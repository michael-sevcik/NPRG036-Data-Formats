            "tableSchema": {
                "@type": "Schema",
                "aboutUrl": "exS:{streetId}",
                "columns": [
                    {
                        "@type": "Column",
                        "datatype": "string",
                        "dc:description": "Street ID",
                        "name": "streetId",
                        "propertyUrl": "rdf:type",
                        "required": true,
                        "titles": "streetId",
                        "valueUrl": "http://example.org/vocabulary/Street"
