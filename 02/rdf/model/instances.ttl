@prefix ex: <http://example.org/vocabulary/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> . 
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix dbo: <http://dbpedia.org/resource/classes#>.



# S P O I L E R
# content of this file will be moved into data.ttl
# this file exists for visual purposes only



# C L A S S E S


# Towns
<ex:Rovinka> a dbo:Town;
    ex:townName "Rovinka"@sk;
    ex:hasFlag "true"^^xsd:boolean;
    ex:hasBroadcast "true"^^xsd:boolean.

<ex:SvatyJur> a dbo:Town;
    ex:townName "Svätý Jur"@sk;
    ex:hasFlag "false"^^xsd:boolean;
    ex:hasBroadcast "false"^^xsd:boolean.

<ex:Zlin> a dbo:Town;
    ex:townName "Zlín"@cs;
    ex:hasFlag "false"^^xsd:boolean;
    ex:hasBroadcast "true"^^xsd:boolean.


# Houses TODO
<ex:MartinsHouse> a dbo:Building;
    ex:houseNumber "123"^^xsd:int.
    # TODO colour
    # TODO are

<ex:MichalsHouse> a dbo:Building;
    ex:houseNumber "5656"^^xsd:int.
    # TODO colour
    # TODO are

<ex:RastosHouse> a dbo:Building;
    ex:houseNumber "466"^^xsd:int.
    # TODO colour
    # TODO are


# Streets
<ex:Hlavna> a dbo:Street;
    ex:streetName "Hlavná"@sk;
    ex:hasSidewalk "true"^^xsd:boolean.

<ex:Sportova> a dbo:Street;
    ex:streetName "Športová"@sk;
    ex:hasSidewalk "false"^^xsd:boolean.

<ex:Mramorova> a dbo:Street;
    ex:streetName "Mramorová"@sk;
    ex:hasSidewalk "true"^^xsd:boolean.


# People
<ex:Michal> a foaf:Person;
    foaf:name "Michal"@cs;
    foaf:surname "Ševčík"@cs;
    ex:middleNames {
        "Dobrodej"@sk
        "Alexander"@sk
    }.

<ex:Rasto> a foaf:Person;
    foaf:name "Rastislav"@sk.

<ex:Martin> a foaf:Person;
    foaf:name "Martin"@sk.


# Occupants
<ex:Michal> a ex:Occupant;
    ex:dateFrom "2018-09-01"^^xsd:date;
    ex:unpaidFees "false"^^xsd:boolean.


# Mayors
<ex:Rasto> a ex:Mayor;
    ex:dateFrom "2018-08-01"^^xsd:date;
    ex:until "2022-08-01"^^xsd:date.

<ex:Martin> a ex:Mayor.

<ex:Michal> a ex:Mayor.


# A S S O C I A T I O N S


# mayor
<ex:Rovinka> ex:mayor <ex:Rasto>.

<ex:SvatyJur> ex:mayor <ex:Martin>.

<ex:Zlin> ex:mayor <ex:Michal>.

# lives_in
<ex:Martin> ex:lives_in <ex:MartinsHouse>.

<ex:Rasto> ex:lives_in <ex:RastosHouse>.

<ex:Michal> ex:lives_in <ex:MichalsHouse>.

# town_hall
<ex:MartinsHouse> ex:town_hall <ex:SvatyJur>.

<ex:RastosHouse> ex:town_hall <ex:Rovinka>.

<ex:MichalsHouse> ex:town_hall <ex:Zlin>.

# locatedIn
<ex:Hlavna> ex:locatedIn <ex:Rovinka>.

<ex:Sportova> ex:locatedIn <ex:SvatyJur>.

<ex:Mramorova> ex:locatedIn <ex:Zlin>.

# situatedoN
<ex:MichalsHouse> ex:situatedOn <ex:Mramorova>.

<ex:RastosHouse> ex:situatedOn <ex:Hlavna>.

<ex:MartinsHouse> ex:situatedOn <ex:Sportova>.