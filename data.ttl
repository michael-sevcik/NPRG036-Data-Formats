@prefix ex: <http://example.org/vocabulary/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> . 
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:Mayor rdfs:subClassOf foaf:Person;
    rdfs:label "Starosta"@cs.

ex:dateFrom a rdf:Property ;
	rdfs:label "Dátum počiatku mandátu"@cs ;
	rdfs:domain ex:Mayor ;
	rdfs:range xsd:date.

ex:until a rdf:Property ;
	rdfs:label "Dátum ukončenia mandátu"@cs ;
	rdfs:domain ex:Mayor ;
	rdfs:range xsd:date.

ex:mayor a rdf:Property ;
	rdfs:label "Starosta mesta"@cs ;
	rdfs:domain ex:Mayor ;
	rdfs:range ex:Town .

#tu ked tak zmenit este typ Town
